{
    "name": "nilportugues/phpserializer",
    "homepage": "http://phpserializer.com",
    "config": {
        "bin-dir": "bin/",
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "twig/twig": "^1.22",
        "vlucas/phpdotenv": "~1.0",
        "rcrowe/twigbridge": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "fabpot/php-cs-fixer": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nilopc/twigbridge"
        }
    ],
    "minimum-stability ": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PhpSerializer\\": "src/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
